{
  "name": "badminton-bot",
  "version": "1.1.1",
  "description": "Messenger Bot for badminton money management",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "api": "node src/api-server.js",
    "api:dev": "nodemon src/api-server.js",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:init": "node scripts/init-db.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d --build app",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "echo \"No linting configured\" && exit 0",
    "audit": "npm audit --audit-level moderate",
    "health": "curl -f http://localhost:3100/health || exit 1",
    "deploy:staging": "docker-compose -f docker-compose.staging.yml up -d",
    "deploy:production": "docker-compose -f docker-compose.prod.yml up -d",
    "backup": "./backup-restore.sh backup",
    "restore": "./backup-restore.sh restore",
    "version:update": "node scripts/update-version.js",
    "version:check": "node scripts/check-version.js",
    "docker:check": "./scripts/check-docker-build.sh",
    "deploy:fix": "./scripts/fix-deployment.sh",
    "cicd:check": "./scripts/check-cicd.sh",
    "deployment:status": "node scripts/deployment-status.js",
    "version:cleanup": "node scripts/cleanup-version.js",
    "version:force-cleanup": "node scripts/force-cleanup-version.js",
    "cicd:workflow": "node scripts/check-cicd-workflow.js",
    "swagger:check": "node scripts/check-swagger-docs.js",
    "swagger:summary": "node scripts/swagger-summary.js",
    "swagger:test": "node scripts/test-swagger-server.js",
    "docker:rebuild": "./scripts/rebuild-docker.sh",
    "docker:test": "./scripts/test-docker-build.sh",
    "deps:fix": "node scripts/fix-dependencies.js",
    "docker:tag": "./scripts/docker-tag-version.sh",
    "docker:push": "./scripts/docker-push-version.sh",
    "docker:version": "node scripts/docker-version-manager.js",
    "docker:summary": "node scripts/docker-versioning-summary.js",
    "rollback": "./rollback.sh rollback",
    "health-check": "./health-check.sh health-check",
    "monitor": "./monitor.sh status"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "isomorphic-dompurify": "^2.6.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "prisma": "^5.7.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "webidl-conversions": "^7.0.0",
    "whatwg-url": "^11.0.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0"
  },
  "overrides": {
    "validator": "^13.15.0",
    "express-validator": {
      "validator": "^13.15.0"
    },
    "z-schema": {
      "validator": "13.15.0"
    },
    "@apidevtools/swagger-parser": {
      "z-schema": "^5.0.5"
    },
    "swagger-jsdoc": {
      "@apidevtools/swagger-parser": "^10.0.3"
    }
  },
  "keywords": [
    "messenger",
    "bot",
    "badminton",
    "money-management"
  ],
  "author": "Senior Software Engineer",
  "license": "MIT"
}
